<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/main_rl_root"
    android:background="@color/color_common_page_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/layout_view"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/layout_main_title_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/dp_18"
        android:paddingEnd="@dimen/dp_12">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <TextView
                android:textSize="@dimen/dp_24"
                android:textColor="@color/color_100_black_white"
                android:ellipsize="end"
                android:id="@+id/layout_main_title_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/arg_res_0x7f1100e2"
                android:maxLines="1"
                android:layout_weight="1"
                android:fontFamily="@font/roboto_bold"
                android:textAlignment="viewStart"
                app:layout_constrainedWidth="true"
                app:layout_constraintEnd_toStartOf="@+id/home_pro"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_default="wrap"/>
            <filerecovery.photosrecovery.allrecovery.ui.widget.CustomShapeTextView
                android:textSize="@dimen/sp_12"
                android:textStyle="italic"
                android:textColor="#ffffff"
                android:gravity="center"
                android:layout_gravity="bottom"
                android:id="@+id/home_pro"
                android:paddingTop="2.5dp"
                android:paddingBottom="2.5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_2"
                android:text="@string/arg_res_0x7f1101af"
                android:includeFontPadding="false"
                android:fontFamily="@font/roboto_black"
                android:paddingStart="@dimen/dp_6"
                android:paddingEnd="@dimen/dp_10"
                app:layout_constrainedWidth="true"
                app:layout_constraintBottom_toBottomOf="@+id/layout_main_title_name"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toEndOf="@+id/layout_main_title_name"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/layout_main_iv_billing"
            android:padding="@dimen/dp_6"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_11"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/ic_home_subscription"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/layout_main_iv_self_ad"
            android:padding="@dimen/dp_6"
            android:visibility="gone"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_36"
            android:layout_marginTop="@dimen/dp_11"
            android:layout_marginStart="@dimen/dp_8"
            app:srcCompat="@mipmap/ic_home_gallery2"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/layout_main_title_setting"
            android:padding="@dimen/dp_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_11"
            android:scaleType="fitXY"
            android:layout_marginStart="@dimen/dp_8"
            app:srcCompat="@drawable/ic_home_settings"/>
        <View
            android:id="@+id/main_v_update_warn"
            android:background="@drawable/bg_ff2400_oval"
            android:visibility="gone"
            android:layout_width="@dimen/dp_8"
            android:layout_height="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_6"
            android:layout_marginEnd="@dimen/dp_4"/>
    </LinearLayout>
    <View
        android:background="@drawable/bg_main_title_line"
        android:layout_width="@dimen/dp_16"
        android:layout_height="@dimen/dp_4"
        android:layout_marginTop="@dimen/dp_2"
        android:layout_marginStart="@dimen/dp_18"/>
    <ScrollView
        android:id="@+id/main_scroll"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_weight="1"
        android:paddingStart="@dimen/dp_18"
        android:paddingEnd="@dimen/dp_18">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/layout_main_photo_recovery"
                android:background="@drawable/bg_main_photo_recovery"
                android:paddingTop="@dimen/dp_21"
                android:paddingBottom="@dimen/dp_21"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_4"
                android:layout_alignParentStart="true"
                app:layout_constraintEnd_toStartOf="@+id/layout_main_wa_recovery"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <androidx.appcompat.widget.AppCompatTextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_100_black_white"
                    android:gravity="start|center_vertical"
                    android:id="@+id/tv_photo_recovery"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/arg_res_0x7f1101a6"
                    android:drawablePadding="@dimen/dp_14"
                    android:fontFamily="@font/roboto_bold"
                    android:textAlignment="viewStart"
                    app:drawableStartCompat="@drawable/ic_home_photo"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color_78869d_white60"
                    android:gravity="start"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_6"
                    android:text="@string/arg_res_0x7f110216"
                    android:fontFamily="@font/roboto_medium"
                    android:textAlignment="viewStart"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_photo_recovery"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:id="@+id/layout_main_wa_recovery"
                android:background="@drawable/bg_main_photo_recovery"
                android:paddingTop="@dimen/dp_21"
                android:paddingBottom="@dimen/dp_21"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_centerVertical="true"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_15"
                android:layout_marginStart="@dimen/dp_9"
                android:layout_alignParentStart="true"
                app:layout_constraintBottom_toBottomOf="@+id/layout_main_photo_recovery"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toEndOf="@+id/layout_main_photo_recovery"
                app:layout_constraintTop_toTopOf="@+id/layout_main_photo_recovery">
                <androidx.appcompat.widget.AppCompatTextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_100_black_white"
                    android:gravity="start|center_vertical"
                    android:id="@+id/tv_wa_recovery"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/arg_res_0x7f11013e"
                    android:maxLines="1"
                    android:drawablePadding="@dimen/dp_14"
                    android:fontFamily="@font/roboto_bold"
                    android:textAlignment="viewStart"
                    app:autoSizeMaxTextSize="@dimen/sp_18"
                    app:autoSizeMinTextSize="@dimen/sp_10"
                    app:autoSizeTextType="uniform"
                    app:drawableStartCompat="@drawable/ic_home_message"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color_78869d_white60"
                    android:gravity="start"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_6"
                    android:text="@string/arg_res_0x7f110237"
                    android:fontFamily="@font/roboto_medium"
                    android:textAlignment="viewStart"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_wa_recovery"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <TextView
                android:textSize="@dimen/sp_14"
                android:textColor="@color/color_100_ffffff"
                android:gravity="center"
                android:id="@+id/main_tv_wa_un_read_count"
                android:background="@drawable/bg_wa_main_un_read_count"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto_bold"
                app:layout_constraintEnd_toEndOf="@+id/layout_main_wa_recovery"
                app:layout_constraintTop_toTopOf="@+id/layout_main_wa_recovery"/>
            <TextView
                android:id="@+id/main_tv_wa_upgrade_dot"
                android:background="@drawable/bg_ff2400_oval"
                android:visibility="gone"
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:layout_margin="@dimen/dp_16"
                app:layout_constraintEnd_toEndOf="@+id/layout_main_wa_recovery"
                app:layout_constraintTop_toTopOf="@+id/layout_main_wa_recovery"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_100_black_white"
                android:gravity="start|center_vertical"
                android:id="@+id/tv_video_recovery"
                android:background="@drawable/bg_white_181818_radio_27"
                android:paddingTop="@dimen/dp_16"
                android:paddingBottom="@dimen/dp_16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/arg_res_0x7f11016b"
                android:drawablePadding="@dimen/dp_14"
                android:drawableStart="@drawable/ic_home_video"
                android:fontFamily="@font/roboto_bold"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_10"
                app:layout_constraintEnd_toStartOf="@+id/tv_audio_recovery"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layout_main_photo_recovery"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_100_black_white"
                android:gravity="start|center_vertical"
                android:id="@+id/tv_audio_recovery"
                android:background="@drawable/bg_white_181818_radio_27"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="@string/arg_res_0x7f110038"
                android:drawablePadding="@dimen/dp_14"
                android:fontFamily="@font/roboto_black"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_9"
                app:drawableStartCompat="@drawable/ic_home_audio"
                app:fontFamily="@font/roboto_bold"
                app:layout_constraintBottom_toBottomOf="@+id/tv_video_recovery"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toEndOf="@+id/tv_video_recovery"
                app:layout_constraintTop_toTopOf="@+id/tv_video_recovery"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_100_black_white"
                android:gravity="start|center_vertical"
                android:id="@+id/tv_document_recovery"
                android:background="@drawable/bg_white_181818_radio_27"
                android:paddingTop="@dimen/dp_16"
                android:paddingBottom="@dimen/dp_16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/arg_res_0x7f1101e4"
                android:drawablePadding="@dimen/dp_14"
                android:fontFamily="@font/roboto_bold"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_10"
                app:drawableStartCompat="@drawable/ic_home_document"
                app:layout_constraintEnd_toStartOf="@+id/tv_contact_recovery"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_video_recovery"/>
            <TextView
                android:id="@+id/tv_document_upgrade_dot"
                android:background="@drawable/bg_red_ff2400_oval_8"
                android:visibility="gone"
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_marginEnd="@dimen/dp_16"
                app:layout_constraintEnd_toEndOf="@+id/tv_document_recovery"
                app:layout_constraintTop_toTopOf="@+id/tv_document_recovery"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_100_black_white"
                android:gravity="start|center_vertical"
                android:id="@+id/tv_contact_recovery"
                android:background="@drawable/bg_white_181818_radio_27"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="@string/arg_res_0x7f110103"
                android:drawablePadding="@dimen/dp_14"
                android:fontFamily="@font/roboto_bold"
                android:textAlignment="viewStart"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_9"
                app:drawableStartCompat="@drawable/ic_home_contact"
                app:layout_constraintBottom_toBottomOf="@+id/tv_document_recovery"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintStart_toEndOf="@+id/tv_document_recovery"
                app:layout_constraintTop_toTopOf="@+id/tv_document_recovery"/>
            <TextView
                android:id="@+id/tv_contact_upgrade_dot"
                android:background="@drawable/bg_red_ff2400_oval_8"
                android:visibility="gone"
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_marginEnd="@dimen/dp_16"
                app:layout_constraintEnd_toEndOf="@+id/tv_contact_recovery"
                app:layout_constraintTop_toTopOf="@+id/tv_contact_recovery"/>
            <LinearLayout
                android:gravity="start|center_vertical"
                android:orientation="horizontal"
                android:id="@+id/layout_main_cleaner"
                android:background="@drawable/bg_white_181818_radio_35"
                android:paddingTop="@dimen/dp_18"
                android:paddingBottom="@dimen/dp_18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_22"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_document_recovery">
                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/layout_main_clean_complete_lottie_anim"
                    android:visibility="visible"
                    android:layout_width="@dimen/dp_24"
                    android:layout_height="@dimen/dp_24"
                    android:scaleType="fitXY"
                    app:lottie_autoPlay="false"
                    app:lottie_rawRes="@raw/anim_clean_completed"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_100_black_white"
                    android:gravity="start"
                    android:id="@+id/main_tv_cleaner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/arg_res_0x7f1100ea"
                    android:layout_weight="1"
                    android:fontFamily="@font/roboto_bold"
                    android:textAlignment="viewStart"
                    android:layout_marginStart="@dimen/dp_14"/>
                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/progress_cleaner_loading"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp_24"
                    android:layout_height="@dimen/dp_24"
                    app:layout_constraintTop_toBottomOf="@+id/tv_scanning_current_count"
                    app:lottie_autoPlay="true"
                    app:lottie_rawRes="@raw/common_progressbar_small"
                    app:lottie_repeatCount="-1"/>
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color_100_F1392F"
                    android:gravity="start"
                    android:id="@+id/tv_cleaner_found_size"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_medium"
                    android:textDirection="locale"
                    android:textAlignment="viewStart"/>
            </LinearLayout>
            <filerecovery.photosrecovery.allrecovery.view.CleanerItemView
                android:gravity="center_vertical"
                android:id="@+id/cleaner_screenshots"
                android:background="@drawable/bg_white_181818_radio_35"
                android:paddingTop="@dimen/dp_18"
                android:paddingBottom="@dimen/dp_18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_22"
                app:cleanerFileSize="22M"
                app:cleanerIcon="@drawable/ic_home_screenshots"
                app:cleanerIconMarginEnd="@dimen/dp_14"
                app:cleanerRightTextColor="@color/color_100_F1392F"
                app:cleanerShowProgress="false"
                app:cleanerTitle="@string/arg_res_0x7f110226"
                app:layout_constraintTop_toBottomOf="@+id/layout_main_cleaner"/>
            <LinearLayout
                android:gravity="start|center_vertical"
                android:orientation="horizontal"
                android:id="@+id/layout_main_result"
                android:background="@drawable/bg_white_181818_radio_35"
                android:paddingTop="@dimen/dp_18"
                android:paddingBottom="@dimen/dp_18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:paddingStart="@dimen/dp_22"
                android:paddingEnd="@dimen/dp_22"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cleaner_screenshots">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/layout_main_result_icon"
                    android:layout_width="@dimen/dp_24"
                    android:layout_height="@dimen/dp_24"
                    android:scaleType="fitCenter"
                    app:srcCompat="@drawable/ic_home_recovered"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/color_100_black_white"
                    android:id="@+id/main_tv_recovered"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/arg_res_0x7f1101bd"
                    android:fontFamily="@font/roboto_bold"
                    android:layout_marginStart="@dimen/dp_14"/>
            </LinearLayout>
            <RelativeLayout
                android:gravity="center_vertical"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal"
                android:id="@+id/rly_main_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_28"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layout_main_result">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/iv_question_mark"
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    android:layout_centerVertical="true"
                    app:srcCompat="@drawable/ic_home_feedback"/>
                <TextView
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color_home_feedback_txt"
                    android:id="@+id/tv_feedback"
                    android:paddingBottom="@dimen/dp_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/arg_res_0x7f1100d6"
                    android:includeFontPadding="false"
                    android:fontFamily="@font/roboto_regular"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_toEndOf="@+id/iv_question_mark"/>
                <View
                    android:background="@color/color_home_feedback_txt"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_feedback"
                    android:layout_alignStart="@+id/tv_feedback"
                    android:layout_alignEnd="@+id/tv_feedback"/>
            </RelativeLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/layout_global_banner_ad"
        android:background="@color/color_100_white_262626"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_alignParentBottom="true"/>
</LinearLayout>
